{
  "name": "node-env-run",
  "version": "1.0.0",
  "description": "Wrapper executable to load env variables from .env and run Node",
  "repository": {
    "url": "git@github.com:dkundel/node-env-run.git",
    "type": "git"
  },
  "author": "Dominik Kundel <dominik.kundel@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/commander": "^2.9.0",
    "@types/debug": "^0.0.29",
    "@types/dotenv": "^4.0.0",
    "@types/node": "^7.0.18",
    "commander": "^2.9.0",
    "debug": "^2.6.7",
    "dotenv": "^4.0.0"
  },
  "bin": {
    "node-env-run": "./dist/bin/node-env-run.js"
  },
  "types": "./dist/bin/node-env-run.d.ts",
  "scripts": {
    "prepublish": "npm run tsc",
    "tsc": "tsc",
    "build": "npm run tsc",
    "test": "jest",
    "travis": "npm run build && npm run test"
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "jest": "^20.0.3",
    "npm-run-all": "^4.0.2",
    "ts-jest": "^20.0.3",
    "typescript": "^2.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
